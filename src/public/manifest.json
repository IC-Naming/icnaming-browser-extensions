{
  "manifest_version": 2,
  "name": "IC naming extensions",
  "description": "Extensions to help you visit icnaming from you browser",
  "version": "1.1.0",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://*.icp/*",
    "*://*.ticp/*"
  ]
}
